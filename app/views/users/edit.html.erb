<h1>Edit My Page</h1> <br>

<%= form_for @user do |f| %>
<p>
    <%= f.label :name %> <br>
    <%= f.text_field :name %>
</p> <br>
<p>
    <%= f.label :username %> <br>
    <%= f.text_field :username %>
</p> <br>
<p>
    <%= f.label :email %> <br>
    <%= f.text_field :email %>
</p> <br>
<p>
    <%= f.label :party %> <br>
    <%= f.text_field :party %>
</p> <br>

<p>
    <%= f.label :district %> <br>
    <%= f.text_field :district %>
</p> <br>


<p class="button"><%= f.submit %></p>
<% end %>